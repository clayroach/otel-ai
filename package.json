{
  "name": "@otel-ai/platform",
  "version": "0.1.0",
  "description": "AI-native observability platform built in 30 days",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --config vitest.integration.config.ts",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "// Infrastructure commands": "",
    "infra:up": "docker-compose up -d",
    "infra:down": "docker-compose down",
    "infra:logs": "docker-compose logs -f",
    "infra:reset": "docker-compose down -v && docker-compose up -d",
    "// Development environment": "",
    "dev:setup": "pnpm install && docker-compose up -d",
    "dev:container": "docker-compose -f docker-compose.dev.yaml run --rm dev",
    "dev:build": "docker build -t otel-ai:dev --target development -f Dockerfile.dev .",
    "dev:shell": "docker-compose -f docker-compose.dev.yaml exec dev /bin/bash",
    "// Code generation": "",
    "generate": "tsx scripts/generate-from-specs.ts",
    "generate:storage": "tsx scripts/generate-from-specs.ts storage",
    "sync:specs": "tsx scripts/sync-specs.ts",
    "// Combined workflows": "",
    "setup": "run-s infra:up install",
    "dev:full": "run-p infra:up dev",
    "ci": "run-s format:check lint typecheck test",
    "release": "run-s ci build"
  },
  "dependencies": {
    "effect": "^3.8.0",
    "@effect/platform": "^0.58.0",
    "@effect/schema": "^0.68.0",
    "@clickhouse/client": "^1.5.0",
    "@aws-sdk/client-s3": "^3.658.0",
    "@opentelemetry/otlp-transformer": "^0.52.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/resources": "^1.25.0",
    "@opentelemetry/semantic-conventions": "^1.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "testcontainers": "^10.11.0",
    "@testcontainers/clickhouse": "^10.11.0",
    "@testcontainers/minio": "^10.11.0",
    "npm-run-all2": "^6.2.0",
    "rimraf": "^5.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0",
    "concurrently": "^8.2.0",
    "chalk": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clayroach/otel-ai.git"
  },
  "keywords": [
    "observability",
    "opentelemetry",
    "ai",
    "machine-learning",
    "clickhouse",
    "typescript",
    "effect-ts"
  ],
  "author": "Clay Roach",
  "license": "MIT"
}
